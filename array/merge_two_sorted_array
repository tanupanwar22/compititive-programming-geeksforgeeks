#include<bits/stdc++.h>
using namespace std;
int main()
 {
     int t;
     cin>>t;
     while(t--)
     {
         int n,m,k;
         cin>>n>>m;
         k=n+m;
         int a[n],b[m],c[k];
         for(int i=0;i<n;i++)
         {
             cin>>a[i];
             c[i]=a[i];
         }
         int j=0;
         for(int i=n;i<n+m;i++)
         {
             cin>>b[j];
             c[i]=b[j];
             j++;
         }
         sort(c,c+k);
         for(int i=0;i<n+m;i++)
         cout<<c[i]<<" ";
         
         cout<<endl;
        
         
         
     }
	//code
	return 0;
}
